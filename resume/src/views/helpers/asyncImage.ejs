<script>
(() => {
    Array.from(document.getElementsByClassName('asyncImage')).map((item) => {
      item.classList.remove('asyncImage')
      const img = new Image()
      img.src = item.attributes.asyncImage.value
      item.src = img.src.replace('.', '-min.')
      img.onload = () => item.src = img.src
    })
  })()
  </script>