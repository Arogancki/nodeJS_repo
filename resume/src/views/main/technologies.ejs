<div class="technologiesContainer">
    <% Object.keys(technologies).forEach((v,i)=> {
        let typeKey = technologies.sort[i] %>
        <div>
            <h1>
                <%= translation.main.technologies[typeKey] %>
                <!-- <i class="material-icons" style="font-size: 3rem;"><%= technologies.icons[typeKey] %></i> -->
            </h1>
            <h4><%= translation.main.technologies[typeKey+"Text"] %></h4>
            <div class="links">
                <% technologies[typeKey].forEach(technology=>{ %>
                    <% if (technology.link) { %>
                        <a target="_blank" rel="noopener noreferrer" href="<%= technology.link %>">
                    <% } %>
                        <div>
                            <span><img src="<%= technology.img %>"></img></span>
                            <p><%= translation.glossary[technology.name] || technology.name %></p>
                        </div>
                    <% if (technology.link) { %>
                        </a>
                    <% } %>
                <% }) %>
            </div>
        </div>
    <% }) %>
</div>