<img class="background asyncImage" asyncImage="/background/.jpg"></img>
<div class="experienceContainer">
    <% experience.forEach((val)=> { %>
        <div>
            <div class="imageContainer">
                <img src="<%= val.img %>"></img>
            </div>
            <div class="details">
                <h1><%= translation.main.experience[val.name] || val.name %></h1>
                <h5> 
                    <i class="material-icons">assignment_ind</i> 
                    <%= translation.main.experience[val.title] || val.title %>
                </h5>
                <h5> 
                    <i class="material-icons">access_time</i>
                    <%= translation.main.experience.from %> 
                    <%= val.from %>
                    <%= translation.main.experience.to %> 
                    <%= val.to || translation.main.experience.currently %>
                </h5>
                <div>
                    <%= translation.main.experience[val.short] || val.desc %>
                    <br>
                    <a href="<%= val.link %>" target="_blank" rel="noopener noreferrer">
                        <i class="material-icons">navigate_next</i>link
                    </a>
                </div>
            </div>
        </div>
    <% }) %>
</div>